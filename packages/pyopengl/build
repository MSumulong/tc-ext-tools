#
# Defines build functions: configure, compile, install
#

build_install() {

  python setup.py install --prefix=/usr/local --root="$INSTALL_ROOT" || return 1

}

build_rules() {

  tet_download
  tet_convert
  tet_unpack
  tet_patch
  tet_load
  tet_install
  tet_split
  tet_strip
  tet_create
  tet_generate
  tet_check
   tet_test
  tet_package
   tet_encrypt

}
