#    package build functions: configure, compile, install, rules, and custom functions     #
#                                                                                          #
############################################################################################

## install
build_install() {

   ## cmake
   make DESTDIR="$I_ROOT" install || return 1
   }

## rules
build_rules() {

   tet_load
   tet_download
   tet_convert
   tet_unpack
   tet_patch
   tet_install
   tet_split
   tet_strip
   tet_create
   tet_generate
   tet_check
   tet_test
   tet_package

}

