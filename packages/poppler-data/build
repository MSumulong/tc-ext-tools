#
# tc-ext-tools build functions: configure, compile, install, rules
#

### Install
build_install() {

  make prefix=/usr/local DESTDIR="$INSTALL_ROOT" install || return 1

}
##

### Rules
build_rules() {

  tet_load
  tet_download
  tet_convert
  tet_unpack
  tet_install
  tet_split
  tet_create
  tet_generate
  tet_check
  tet_test
  tet_package
  tet_encrypt

}
##
