
############################################################################################
#                                                                                          #
#    package build functions: configure, compile, install, rules, and custom functions     #
#                                                                                          #
############################################################################################

## install
build_install() {

   install -d $I_ROOT/usr/local/share/tmw/data/music
   install --mode=644 data/music/* $I_ROOT/usr/local/share/tmw/data/music

}

## rules
build_rules() {

   tet_load
   tet_download
   tet_convert
   tet_unpack
   tet_install
   tet_split
   tet_create
   tet_generate
   tet_check
   tet_package

}

