### Install
build_install() {

  sudo python setup.py install --prefix=/usr/local --root=${INSTALL_ROOT} || return 1

}
##

### Rules
build_rules() {

  tet_download
  tet_convert
  tet_unpack
  tet_patch
  tet_load
  tet_configure
  tet_compile
  tet_install
  tet_split
   tet_strip
  tet_create
  tet_generate
  tet_check
  tet_package

}
