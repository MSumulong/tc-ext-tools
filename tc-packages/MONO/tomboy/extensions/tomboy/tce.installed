#!/bin/sh

[ -x $(which gtk-update-icon-cache) ] && gtk-update-icon-cache -q -f -t /usr/local/share/icons/hicolor > /dev/null 2>&1
[ -x $(which update-gconf-database) ] && update-gconf-database register tomboy > /dev/null 2>&1
[ -x $(which update-mime-database) ] && update-mime-database /usr/local/share/mime > /dev/null 2>&1

